    "@nestjs/swagger": "^5.0.9",
    "@nestjs/websockets": "^8.4.3",
    "aws-sdk": "^2.974.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "connect-redis": "^6.0.0",
