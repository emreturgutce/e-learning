    "helmet": "^4.6.0",
    "ioredis": "^4.27.8",
    "mongoose": "^5.13.7",
    "nanoid": "^3.1.25",
    "nest-winston": "^1.5.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sharp": "^0.29.0",
    "swagger-ui-express": "^4.1.6",
    "winston": "^3.3.3"
